<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <GenerateProgramFile>true</GenerateProgramFile>
    </PropertyGroup>
    <ItemGroup>
        <Content Include="wwwroot\**\*">
            <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
        </Content>
    </ItemGroup>
    <ItemGroup>
        <Compile Include="Tools\FormDataBuilder.fs" />
        <Compile Include="Tools\TestServer.fs" />
    </ItemGroup>
    <ItemGroup>
        <Compile Include="E2E\KestrelTestServer.fs" />
        <Compile Include="E2E\E2EPlaywright.fs" />
        <Compile Include="Navigation.fs" />
        <Compile Include="Login.fs" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="AngleSharp" Version="1.2.0" />
        <PackageReference Include="Microsoft.NET.Test.Sdk" Version="17.12.0" />
        <PackageReference Include="Microsoft.Playwright" Version="1.49.0" />
        <PackageReference Include="xunit" Version="2.9.3" />
        <PackageReference Include="FsUnit.xUnit" Version="6.0.1" />
        <PackageReference Include="Microsoft.AspNetCore.Mvc.Testing" Version="9.0.1" />
        <PackageReference Include="xunit.runner.visualstudio" Version="3.0.1">
          <PrivateAssets>all</PrivateAssets>
          <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
        <PackageReference Update="FSharp.Core" Version="9.0.101" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\Web\Web.fsproj" />
    </ItemGroup>
    <ItemGroup>
      <Content Include="OrganizationsFromExcel\db_sample.xlsx" />
      <Compile Include="OrganizationsFromExcel\Tests.fs" />
      <Content Include="OrganizationsFromExcel\db_sample.csv">
        <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      </Content>
    </ItemGroup>
</Project>