package steps

import "charity_portal/charity_update/queries"

templ BeneficenciForm(data queries.Beneficjenci, teczkaId int64) {
<form>
  <fieldset class="two-col-form">
    <label for="Beneficjenci">Beneficjenci</label>
    <input name="Beneficjenci" id="Beneficjenci" required value={ data.Beneficjenci } />
  </fieldset>

  <input class="secondary" type="submit" value="Zapisz" hx-put="/charity-update/kontakty" hx-target="#wizard" hx-push-url="true" />
</form>
}
